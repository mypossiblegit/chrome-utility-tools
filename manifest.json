{
  "manifest_version": 3,
  "name": "Chrome Utility Tools",
  "version": "1.2",
  "description": "A modular Chrome extension for utility tools.",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
